{
    "name": "chunge/laravel",
    "version": "1.0.6",
    "description": "用于laravel框架在初次安装的时候建立基本的运行结构以便快熟入手使用",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "files": [
            "src/Support/Helper.php"
        ],
        "psr-4": {
            "Chunge\\Laravel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "chunge-php"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3",
        "composer/installers": "^2.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Chunge\\Laravel\\Installer::copyFiles"
        ],
        "post-update-cmd": [
            "Chunge\\Laravel\\Installer::copyFiles"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}