{
    "name": "chunge/laravel",
    "description": "用于laravel框架在初次安装的时候建立基本的运行结构以便快熟入手使用",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Chunge\\Laravel\\": "src/"
        },
        "files": [
            "app/Support/Helper.php"
        ]
    },
    "authors": [
        {
            "name": "chunge-php"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": "^7.3",
        "composer/installers": "^2.0@dev"
    },
    "scripts": {
        "post-install-cmd": [
            "ChungeLaravel::postInstall",
            "cp src/Support/Helper.php ../../app/Support/"
        ],
        "post-update-cmd": [
            "cp src/Support/Helper.php ../../app/Support/"
        ]
    },
    "extra": {
        "installer-paths": {
            "../../app/Support/": [
                "chunge/laravel"
            ]
        },
        "autoload": {
            "files": [
                "../../app/Support/Helper.php"
            ]
        }
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}