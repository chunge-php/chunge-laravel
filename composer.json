{
    "name": "chunge/laravel",
    "version": "2.1.0",
    "description": "用于laravel框架在初次安装的时候建立基本的运行结构以便快速入手使用",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "files": [
            "src/setup.php"
        ],
        "psr-4": {
            "Chunge\\Laravel\\": "src/"
        }
    },
    "authors": [
        {
            "name": "chunge-php",
            "email": "drq.chunge.php@qq.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.3|^8.0",
        "composer/installers": "^2.0",
        "laravel/framework": "^8.0"
    },
    "scripts": {
        "post-install-cmd": [
            "Chunge\\Laravel\\InstallCmd::send"
        ],
        "post-update-cmd": [
            "Chunge\\Laravel\\InstallCmd::send"
        ],
        "post-package-update": [
            "Chunge\\Laravel\\InstallCmd::send"
        ],
        "post-package-install": [
            "Chunge\\Laravel\\InstallCmd::send"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true
        }
    }
}